    "es6-shim": "^0.35.0",
    "font-awesome": "^4.5.0",
    "highlight.js": "^9.2.0",
    "lodash": "^4.3.0",
    "marked": "^0.3.5",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
