{
  "name": "cafe-pitch",
  "version": "0.0.3",
  "description": "Markdown-driven presentation tool built on Electron.",
  "main": "dist/browser/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe-re/cafe-pitch.git"
  },
  "bin": {
    "cafepitch": "./bin/cli.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "prepare": "gulp build:prepare && tsconfig -u tsconfig.json",
    "build": "npm run prepare && tsc",
    "watch": "npm run prepare && concurrently --kill-others \"tsc -w\" \"gulp watch\"",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
